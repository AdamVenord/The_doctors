<h1>Add Office</h1>
<%= form_with(model: @office, local: true, class: "ui form" ) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, 
      required: true, 
      placeholder: "Office Name" 
    %>
  </div>
  <%= f.submit nil, class: "ui blue button inverted" %>
<% end %>